<complexType name='HttpBasicHeaderFields'>
    <!-- This is a combination of HTTP General header fields and HTTP Entity
         header fields -->
   
<sequence>
    <!-- General header fields --> 
    <element name='Cache-Control' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Connection' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Date' type ='dateTime' minOccurs='0' maxOccurs='1'/>
    <element name='Pragma' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Trailer' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Transfer-Encoding' type ='http:HttpTransferEncoding' minOccurs='0' maxOccurs='unbounded'/>
    <element name='Upgrade' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Via' type ='http:HttpViaRecord' minOccurs='0' maxOccurs='unbounded'/>
    <element name='Warning' type ='string' minOccurs='0' maxOccurs='1'/>

    <!-- Entity header fields -->
    <element name='Allow' type ='http:HttpRequestMethod' minOccurs='0' maxOccurs='unbounded'/>
    <element name='Content-Encoding' type ='string' minOccurs='0' maxOccurs='unbounded'/>
    <element name='Content-Language' type ='string' minOccurs='0' maxOccurs='unbounded'/>
    <element name='Content-Length' type ='int' minOccurs='0' maxOccurs='1'/>
    <element name='Content-Location' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Content-MD5' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Content-Range' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Content-Type' type ='http:HttpContentType' minOccurs='0' maxOccurs='1'/>
    <element name='Expires' type ='dateTime' minOccurs='0' maxOccurs='1'/>
    <element name='Last-Modified' type ='dateTime' minOccurs='0' maxOccurs='1'/>
</sequence>
</complexType>

<complexType name='HttpRequestHeaderFields'>
<sequence>
    <element name='Accept' type ='string' minOccurs='0' maxOccurs='unbounded'/>
    <element name='Accept-Charset' type ='string' minOccurs='0' maxOccurs='unbounded'/>
    <element name='Accept-Encoding' type ='string' minOccurs='0' maxOccurs='unbounded'/>
    <element name='Accept-Language' type ='string' minOccurs='0' maxOccurs='unbounded'/>
    <element name='Authorization' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Expect' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='From' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Host' type ='http:HttpHost' minOccurs='0' maxOccurs='1'/>
    <element name='If-Match' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='If-Modified-Since' type ='dateTime' minOccurs='0' maxOccurs='1'/>
    <element name='If-None-Match' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='If-Range' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='If-Unmodified-Since' type ='dateTime' minOccurs='0' maxOccurs='1'/>
    <element name='Max-Forwards' type ='int' minOccurs='0' maxOccurs='1'/>
    <element name='Proxy-Authorization' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Range' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Referer' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='TE' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='User-Agent' type ='string' minOccurs='0' maxOccurs='1'/>
</sequence>
</complexType>

<complexType name='HttpResponseHeaderFields'>
<sequence>
    <element name='Accept-Ranges' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Age' type ='int' minOccurs='0' maxOccurs='1'/>
    <element name='ETag' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Location' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Proxy-Authenticate' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Retry-After' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Server' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='Vary' type ='string' minOccurs='0' maxOccurs='1'/>
    <element name='WWW-Authenticate' type ='string' minOccurs='0' maxOccurs='1'/>
</sequence>
</complexType>

